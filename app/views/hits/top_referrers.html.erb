<h1>Top Referrers</h1>

<a href="#/hits">Home</a>
<a href="#/top/urls">Top URLs</a>

<div ng-repeat="(date, records) in top_referrers">
  <h3>{{ date }}</h3>
  <ul>
    <li ng-repeat="record in records">
      {{ record.url }} - Visits: {{ record.visits }}
    </li>

    <li ng-show="!records.length">No visits for this day</li>
  </ul>
</div>

